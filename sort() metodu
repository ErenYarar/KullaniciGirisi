#Küçükten büyüğe sıralama
liste = [21,38,44,15,72,110,68,44,20]
liste2 = ["Eren","Yarar","on","numara"]


liste2.sort()
liste.sort()
print(liste)
print(liste2)

print("--------------------------")
#Büyükten küçüğe sıralama
liste = [21,38,44,15,72,110,68,44,20]
liste2 = ["Eren","Yarar","on","numara"]

liste2.sort(reverse = True)
liste.sort(reverse = True)
print(liste)
print(liste2)
